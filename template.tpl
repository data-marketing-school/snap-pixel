const log = require('logToConsole');
const injectScript = require('injectScript');
const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const createQueue = require('createQueue');
const copyFromDataLayer = require('copyFromDataLayer');
const Object = require('Object');
const user_data = copyFromDataLayer('user_data');
const ecommerce = copyFromDataLayer('ecommerce');
const level_name = copyFromDataLayer('level_name');
const search_term = copyFromDataLayer('search_term');
const method = copyFromDataLayer('method');
const datalayer_event = copyFromDataLayer('event');

const event_mapping = {
  'gtm.js': 'PAGE_VIEW',
  'gtm.init': 'PAGE_VIEW',
  'gtm.init_consent': 'PAGE_VIEW',
  'gtm.dom': 'PAGE_VIEW',
  'gtm.load': 'PAGE_VIEW',
  'page_view': 'PAGE_VIEW',
  'view_item_list': 'LIST_VIEW',
  'view_item': 'VIEW_CONTENT',
  'add_to_wishlist': 'ADD_TO_WISHLIST',
  'add_to_cart': 'ADD_CART',
  'begin_checkout': 'START_CHECKOUT',
  'add_payment_info': 'ADD_BILLING',
  'purchase': 'PURCHASE',
  'tutorial_complete':'COMPLETE_TUTORIAL',
  'level_end': 'LEVEL_COMPLETE',
  'login': 'LOGIN',
  'search': 'SEARCH',
  'share': 'SHARE',
  'sign_up': 'SIGN_UP',
  'unlock_achievement': 'ACHIEVEMENT_UNLOCKED'
};

function bootstrapFn() {
  var snaptr = copyFromWindow('snaptr');
  if (snaptr) {
    return snaptr;
  }
  
  setInWindow('snaptr', function() {
    var handler = copyFromWindow('snaptr.handleRequest');
    if (handler) {
      callInWindow('snaptr.handleRequest.apply', null, arguments);
    } else {
      callInWindow('snaptr.queue.push', arguments);
    }
  });
  return copyFromWindow('snaptr');
}

function getEventParameters() {
  let event_parameters = {};
  
  if(data.sendEcommerceData) {
    if(ecommerce) {
      event_parameters.transaction_id = ecommerce.transaction_id ? ecommerce.transaction_id : undefined;
      event_parameters.price = ecommerce.value ? ecommerce.value : undefined;
      event_parameters.currency = ecommerce.currency ? ecommerce.currency : undefined;
      event_parameters.number_items = ecommerce.items ? ecommerce.items.length : undefined;
      event_parameters.item_ids = getItemIds();
      event_parameters.item_category = ecommerce.items ? ecommerce.items[0].item_category : undefined;
      event_parameters.brands = getItemBrands();
      event_parameters.delivery_method = ecommerce.shipping_tier ? ecommerce.shipping_tier : undefined;
    }
    
    if(level_name) event_parameters.level = level_name;
    if(search_term) event_parameters.search_string = search_term;
    if(method) event_parameters.sign_up_method = method;
  
  }
  
  if(data.eventParametersTable) {
    data.eventParametersTable.forEach(function(d) {
      event_parameters[d.name] = d.value;
    });
  }
  
  return event_parameters;

}

function getItemIds() {
  
  let item_ids = [];
  
  if(ecommerce.items) {
    ecommerce.items.forEach(function(item){
      item_ids.push(item.item_id);
    });
  }
  
  return item_ids;
  
}

function getItemBrands() {
  
  let item_brands = [];
  
  if(ecommerce.items) {
    ecommerce.items.forEach(function(item){
      item_brands.push(item.item_brand);
    });
  }
  
  return item_brands;
  
}

function getUserParameters() {
  let user_parameters = {};
  
  if(data.sendUserData && user_data) {
    if(user_data.email_address) {
      user_parameters.user_email = user_data.email_address;
    }
    
    if(user_data.sha256_email_address) {
      user_parameters.user_hashed_email = user_data.sha256_email_address;
    }
  
    if(user_data.phone_number) {
      user_parameters.user_phone_number = user_data.phone_number;
    }
    
    if(user_data.sha256_phone_number) {
      user_parameters.user_hashed_phone_number = user_data.sha256_phone_number;
    }
    
    if(user_data.address) {
      
      if(user_data.address.first_name) {
        user_parameters.firstname = user_data.address.first_name;
      }
      
      if(user_data.address.last_name) {
        user_parameters.lastname = user_data.address.last_name;
      }
      
      if(user_data.address.city) {
        user_parameters.geo_city = user_data.address.city;
      }
      
      if(user_data.address.country) {
        user_parameters.geo_country = user_data.address.country;
      }
      
      if(user_data.address.postal_code) {
        user_parameters.geo_postal_code = user_data.address.postal_code;
      }
      
      if(user_data.address.region) {
        user_parameters.geo_region = user_data.address.region;
      }
      
    }
  }
  
  if(data.userParametersTable) {
    data.userParametersTable.forEach(function(d) {
      user_parameters[d.name] = d.value;
    });
  }
  
  return user_parameters;
  
}

function loadSnapScript() {
  const snaptr = bootstrapFn();
  const url = "https://sc-static.net/scevent.min.js";
  const event_name = data.eventName == "datalayer" ? event_mapping[datalayer_event] : data.eventName;
  
  log(event_name);
  
  createQueue('snaptr.queue');

  data.integration = "gtm";
  log("EP = ", getEventParameters());
  log("UP = ", getUserParameters());
  log("data = ", merge(getUserParameters(), getEventParameters()));

  snaptr('init', data.pixelId);
  snaptr('track', data.pixelId, event_name, merge(getUserParameters(), getEventParameters()));

  injectScript(url, data.gtmOnSuccess, data.gtmOnFailure, url);
}

function merge(a, b) {
  
  let mergedObj = {};

  for (let key in a) {
    if (a.hasOwnProperty(key)) {
      mergedObj[key] = a[key];
    }
  }

  for (let key in b) {
    if (b.hasOwnProperty(key)) {
      mergedObj[key] = b[key];
    }
  }

  return mergedObj;
  
}

loadSnapScript();


